var calcForDiscount = function(){
	var val1 = parseFloat($('#discounttestscorecounttmp').val());
	var val2 = parseFloat($('#discountsatsubcounttmp').val());
	var val3 = parseFloat($('#discountactcounttmp').val());
	var val4 = parseFloat($('#discountsatcounttmp').val());
	
	
	var valuc = parseInt($('#discounttotalappcostuc').val());
	var valcsu = parseInt($('#discounttotalappcostcsu').val());


	
	var coacsuCost = parseInt($('#discountcoacsu').val());
	var coaucCost = parseInt($('#discountcoauc').val());


	//var transFee=parseInt($('#schooltransfee').val());
	
	
	//return val1+val2+val3+val4+valuc+valcsu+coacsuCost+coaucCost + transFee;
	return val1+val2+val3+val4+valuc+valcsu+coacsuCost+coaucCost;
}

var calcALL = function(){

	var val1 = parseFloat($('#testscorecounttmp').val());
	var val2 = parseFloat($('#satsubcounttmp').val());
	var val3 = parseFloat($('#actcounttmp').val());
	var val4 = parseFloat($('#satcounttmp').val());
	
	var valuc = parseInt($('#totalappcostuc').val());
	var valcsu = parseInt($('#totalappcostcsu').val());
	$('#totalappcost').val(valuc+valcsu);
	
	var coacsuCost = parseInt($('#coacsu').val());
	var coaucCost = parseInt($('#coauc').val());

	//transFee=parseInt($('#schooltransfee').val());
	
	totalTesting = val1+val2+val3+val4;
	$('#totaltestingcost').val(totalTesting);
	
	totalAppCosts = parseInt($('#totalappcost').val());
	
	totalCOA = coacsuCost+coaucCost;
	
	totalDiscountCost = calcForDiscount();
	
	
	//totalOtherCosts = parseInt($('#totalothercost').val());
	
	//total = transFee+totalTesting+totalAppCosts+totalCOA;
	total = totalTesting+totalAppCosts+totalCOA;
	if($("#freelunchyes").is(':checked') || $("#freewaiversyes").is(':checked') ){
		totalOthers = totalDiscountCost - total;
	} else {
		totalOthers = 0;
	}
	$('#maincostval').html(total);
	$('#commcostval').html(totalOthers);
		
}

var totalUCAppCount = function (eventVal){

	var fval = 0;
	var val=eventVal;
 		if($("#freelunchyes").is(':checked') || $("#freewaiversyes").is(':checked')){
			if (val > 0){
				$('#discounttotalappcostuc').val(70+(70*(val-1)));
			}else {
				$('#discounttotalappcostuc').val(0);
			}
 			fval = 70+(70*(val-1))-280;
     	} else{
     		fval = 70+(70*(val-1));
        }
	if(fval<0 || val <= 0){
		fval=0;
	}
	$('#totalappcostuc').val(fval);
	var valuc = parseInt($('#totalappcostuc').val());
	var valcsu = parseInt($('#totalappcostcsu').val());
	$('#totalappcost').val(valuc+valcsu);
}


var totalCSUAppCount = function (eventVal){

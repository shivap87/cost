var calcCOA =  function(evalue){
	$('#CSUSHOW').hide();
	$('#UCSHOW').hide();
	$('#'+evalue+'SHOW').show();
	var liveVAL=parseInt($('#livewith option:selected').val());
	var liveCOST = 0;
	if(evalue=='CSU'){
	if(liveVAL==1){
		liveCOST=$('#residentStayingWithRelativesCSU').val();
	}else if(liveVAL==2){
		liveCOST=$('#residentStayingOnCampusCSU').val();
	}else if(liveVAL==3){
		liveCOST=$('#residentStayingOffCampusCSU').val();
	}else if(liveVAL==4){
		liveCOST=$('#nonresidentStayingWithRelativesCSU').val();
	}else if(liveVAL==5){
		liveCOST=$('#nonresidentStayingOnCampusCSU').val();
	}else if(liveVAL==6){
		liveCOST=$('#nonresidentStayingOffCampusCSU').val();
	}
	}else if(evalue=='UC'){
		if(liveVAL==1){
			liveCOST=$('#residentStayingWithRelativesUC').val();
		}else if(liveVAL==2){
			liveCOST=$('#residentStayingOnCampusUC').val();
		}else if(liveVAL==3){
			liveCOST=$('#residentStayingOffCampusUC').val();
		}else if(liveVAL==4){
			liveCOST=$('#nonresidentStayingWithRelativesUC').val();
		}else if(liveVAL==5){
			liveCOST=$('#nonresidentStayingOnCampusUC').val();
		}else if(liveVAL==6){
			liveCOST=$('#nonresidentStayingOffCampusUC').val();
		}
	}

	

	
	if(evalue=='CSU'){
		if($("#freelunchyes").is(':checked') || $("#freewaiversyes").is(':checked') ){
		$('#coacsu').val(liveCOST-5472);
		$('#coauc').val('0');

		$('#discountcoacsu').val(liveCOST);
		$('#discountcoauc').val('0');

		$('#maincost').html('Total Cost Per Year - ' + evalue);
		//$('#maincostval').html((liveCOST-1500+totaltesting+totalAppCost+schooltransfee));
		}else{
		
			$('#coacsu').val(liveCOST);
			$('#coauc').val('0');
			$('#maincost').html('Total Cost Per Year - ' + evalue);
		//	$('#maincostval').html((liveCOST+totaltesting+totalAppCost+schooltransfee));
		}
	}else if (evalue=='UC'){
		if($("#freelunchyes").is(':checked') || $("#freewaiversyes").is(':checked') ){
		$('#coauc').val(liveCOST-14000);
		$('#coacsu').val('0');
		
		$('#discountcoauc').val(liveCOST);
		$('#discountcoacsu').val('0');
		
		$('#maincost').html('Total Cost Per Year - ' + evalue);
		//$('#maincostval').html((liveCOST-1500+totaltesting+totalAppCost+schooltransfee));
		}else{
			$('#coauc').val(liveCOST);
			$('#coacsu').val('0');
			$('#maincost').html('Total Cost Per Year - ' + evalue);
			//$('#maincostval').html((liveCOST+totaltesting+totalAppCost+schooltransfee));
		}
		
	}
	
}


